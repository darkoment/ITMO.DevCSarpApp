<Window x:Class="WpfHello.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfHello"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas>
        <Button x:Name="button" Content="Set Name" HorizontalAlignment="Left" Margin="125,123,0,0" VerticalAlignment="Top" Width="100" Height="35" Click="Button_Click" />
        <Button x:Name="button1" Content="Ret Name" HorizontalAlignment="Left" Height="35" Margin="125,195,0,0" VerticalAlignment="Top" Width="100" Click="Button_Click_1" />
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="35" Margin="263,123,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="160" TextChanged="textBox_TextChanged"/>
        <Label x:Name="label" Content="Label" HorizontalAlignment="Left" Height="35" Margin="263,195,0,0" VerticalAlignment="Top" Width="350"/>
        <Button Canvas.Left="10" Canvas.Top="10" Content="Click Me" Height="23" Name="button3" Width="75" Click="button3_Click"/>
        <TextBox Width="170" Canvas.Left="90" Canvas.Top="12" Name="myTextBox" />
    </Canvas>
</Window>
