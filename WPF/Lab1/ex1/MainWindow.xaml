        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfHello"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Set Name" HorizontalAlignment="Left" Margin="125,123,0,0" VerticalAlignment="Top" Width="100" Height="35" Click="Button_Click"/>
        <Button Content="Ret Name" HorizontalAlignment="Left" Height="35" Margin="125,195,0,0" VerticalAlignment="Top" Width="100" Click="Button_Click_1"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="35" Margin="263,123,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="160"/>
        <Label x:Name="label" Content="Label" HorizontalAlignment="Left" Height="35" Margin="263,195,0,0" VerticalAlignment="Top" Width="350"/>

    </Grid>
